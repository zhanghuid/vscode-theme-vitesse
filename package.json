{
	"publisher": "huid",
	"name": "my-theme-vitesse",
	"displayName": "My Vitesse Theme",
	"version": "0.0.6",
	"packageManager": "pnpm@9.1.1",
	"description": "Vitesse theme for VS Code",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/zhanghuid/vscode-theme-vitesse"
	},
	"bugs": {
		"url": "https://github.com/zhanghuid/vscode-theme-vitesse/issues"
	},
	"keywords": [
		"theme",
		"vitesse",
		"light",
		"dark",
        "monokai",
        "dracula",
        "material"
	],
	"categories": [
		"Themes"
	],
	"exports": {
		"./*": "./*"
	},
	"icon": "icon.png",
	"files": [
		"extra",
		"themes"
	],
	"engines": {
		"vscode": "^1.43.0"
	},
	"contributes": {
		"themes": [
			{
				"label": "Vitesse Light",
				"uiTheme": "vs",
				"path": "./themes/vitesse-light.json"
			},
			{
				"label": "Vitesse Dark",
				"uiTheme": "vs-dark",
				"path": "./themes/vitesse-dark.json"
			},
			{
				"label": "Vitesse Black",
				"uiTheme": "vs-dark",
				"path": "./themes/vitesse-black.json"
			},
			{
				"label": "Vitesse Light Soft",
				"uiTheme": "vs",
				"path": "./themes/vitesse-light-soft.json"
			},
			{
				"label": "Vitesse Dark Soft",
				"uiTheme": "vs-dark",
				"path": "./themes/vitesse-dark-soft.json"
			},
			{
				"label": "Vitesse Dark Monokai Soft",
				"uiTheme": "vs-dark",
				"path": "./themes/vitesse-dark-monokai-soft.json"
			},
			{
				"label": "Vitesse Dark Dracula Soft",
				"uiTheme": "vs-dark",
				"path": "./themes/vitesse-dark-dracula-soft.json"
			},
            {
                "label": "Vitesse Dark Material Default",
                "uiTheme": "vs-dark",
                "path": "./themes/vitesse-dark-material-default.json"
            }
		]
	},
	"scripts": {
		"build": "esno scripts/index.ts",
		"dev": "esno watch scripts/index.ts",
		"vscode:prepublish": "npm run build",
		"lint": "eslint .",
		"prepublishOnly": "npm run build",
		"release": "bumpp --all && vsce publish --no-dependencies"
	},
	"devDependencies": {
		"@antfu/eslint-config": "^2.19.1",
		"@antfu/utils": "^0.7.8",
		"@types/color": "^3.0.6",
		"@types/fs-extra": "^11.0.4",
		"@types/node": "^20.12.12",
		"@vscode/vsce": "^2.26.1",
		"bumpp": "^9.4.1",
		"color": "^4.2.3",
		"eslint": "^9.3.0",
		"esno": "^4.7.0",
		"fs-extra": "^11.2.0",
		"tsup": "^8.0.2",
		"typescript": "^5.4.5",
		"xterm": "^5.3.0"
	},
	"preview": true
}